<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="fondoSecundaria" stylesheets="@estilos.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="conacyt.SecundariaController">
   <children>
      <Label layoutX="35.0" layoutY="38.0" prefHeight="24.0" prefWidth="713.0" text="¿Cómo se llama el sistema de numeracion que solo usa ceros">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </Label>
      <Label layoutX="35.0" layoutY="62.0" prefHeight="24.0" prefWidth="713.0" text="y unos y que utilizan los ordenadores?">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </Label>
      <RadioButton fx:id="rbhexa" layoutX="81.0" layoutY="148.0" mnemonicParsing="false" onAction="#selecthexa" prefHeight="27.0" prefWidth="269.0" text="Hexadecimal">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbbinario" layoutX="81.0" layoutY="294.0" mnemonicParsing="false" onAction="#selectbinario" prefHeight="27.0" prefWidth="269.0" text="Binario">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbEE" layoutX="81.0" layoutY="243.0" mnemonicParsing="false" onAction="#selectEE" prefHeight="27.0" prefWidth="312.0" text="Español Estructurado">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbdecimal" layoutX="81.0" layoutY="194.0" mnemonicParsing="false" onAction="#selectdecimal" prefHeight="27.0" prefWidth="269.0" text="Decimal">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <Button fx:id="boton8" layoutX="511.0" layoutY="495.0" mnemonicParsing="false" onAction="#selectsiguiente" text="Continuar">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
