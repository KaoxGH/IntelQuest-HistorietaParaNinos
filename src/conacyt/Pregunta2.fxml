<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="fondoSecundaria" stylesheets="@estilos.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="conacyt.SecundariaController">
   <children>
      <Label layoutX="46.0" layoutY="38.0" text="¿Que lenguaje de programación es utilizado">
         <font>
            <Font name="Simplified Arabic Fixed" size="24.0" />
         </font>
      </Label>
      <Label layoutX="46.0" layoutY="144.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="46.0" layoutY="75.0" text="para la creacion y diseño de páginas Web?">
         <font>
            <Font name="Simplified Arabic Fixed" size="24.0" />
         </font>
      </Label>
      <RadioButton fx:id="rbCW" layoutX="46.0" layoutY="211.0" mnemonicParsing="false" onAction="#selectcw" text="C#.windows">
         <font>
            <Font name="Simplified Arabic Fixed" size="24.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbmysql" layoutX="46.0" layoutY="258.0" mnemonicParsing="false" onAction="#selectmysql" text="MySQL">
         <font>
            <Font name="Simplified Arabic Fixed" size="24.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbC" layoutX="46.0" layoutY="309.0" mnemonicParsing="false" onAction="#selectC" text="C++">
         <font>
            <Font name="Simplified Arabic Fixed" size="24.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbphp" layoutX="46.0" layoutY="156.0" mnemonicParsing="false" onAction="#selectphp" text="PHP">
         <font>
            <Font name="Simplified Arabic Fixed" size="24.0" />
         </font>
      </RadioButton>
      <Button fx:id="boton2" layoutX="511.0" layoutY="495.0" mnemonicParsing="false" onAction="#selectsiguiente" text="Continuar">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
