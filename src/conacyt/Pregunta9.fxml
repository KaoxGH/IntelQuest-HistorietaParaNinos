<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="fondoSecundaria" stylesheets="@estilos.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="conacyt.SecundariaController">
   <children>
      <Label layoutX="44.0" layoutY="56.0" prefHeight="24.0" prefWidth="713.0" text="Unidad mínima de información en informatica">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </Label>
      <RadioButton fx:id="rbGB" layoutX="75.0" layoutY="164.0" mnemonicParsing="false" onAction="#selectGB" prefHeight="27.0" prefWidth="269.0" text="GigaByte">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbbyte" layoutX="75.0" layoutY="304.0" mnemonicParsing="false" onAction="#selectbyte" prefHeight="27.0" prefWidth="269.0" text="Byte">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbrom" layoutX="75.0" layoutY="254.0" mnemonicParsing="false" onAction="#selectrom" prefHeight="27.0" prefWidth="269.0" text="ROM">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="rbbit" layoutX="75.0" layoutY="207.0" mnemonicParsing="false" onAction="#selectbit" prefHeight="27.0" prefWidth="269.0" text="Bit">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </RadioButton>
      <Button fx:id="boton9" layoutX="513.0" layoutY="488.0" mnemonicParsing="false" onAction="#selectsiguiente" text="Continuar">
         <font>
            <Font name="Simplified Arabic Fixed" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
